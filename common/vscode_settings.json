{
    /* [Extension List]
        - VIM                   by vscodevim
        - Dolch Theme           by be5invis


    /** 
     * WORKBENCH
     */
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Dolch Light",
    "workbench.preferredLightColorTheme": "Dolch Light",
    "workbench.preferredDarkColorTheme": "Dolch",
    "workbench.iconTheme": "dolch",
    "workbench.editor.labelFormat": "medium",
    "workbench.editor.showIcons": false,
    "workbench.commandPalette.history": 10,

    /**
     * BREADCRUMB
     */
    "breadcrumbs.enabled": false,

    /**
     * EDITOR
     */
    "editor.fontFamily": "'Iosevka Pragmatus",
    "editor.fontSize": 15,
    "editor.fontLigatures": true,
    "editor.codeLens": true,
    "editor.minimap.renderCharacters": false,

    /*
     * WINDOW
     */
    "window.autoDetectColorScheme": true,

    /**
     * EXTENSIONS
     */
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,


    /**
     * EXT:VIM
     */
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": false,
    "vim.normalModeKeyBindingsNonRecursive": [
    ],
}