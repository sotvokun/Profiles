{
    /* [Extension List]
        - VIM                   by vscodevim
        - Dolch Theme           by be5invis
        - C/C++                 by Microsoft
        - Doxygen Documentation by Christoph Schlosser

    /** 
     * WORKBENCH
     */
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Dolch Light",
    "workbench.preferredLightColorTheme": "Dolch Light",
    "workbench.preferredDarkColorTheme": "Dolch",
    "workbench.iconTheme": "dolch",
    "workbench.editor.labelFormat": "medium",
    "workbench.editor.showIcons": false,
    "workbench.commandPalette.history": 10,

    /**
     * BREADCRUMB
     */
    "breadcrumbs.enabled": false,

    /**
     * EDITOR
     */
    "editor.fontFamily": "'Iosevka Pragmatus",
    "editor.fontSize": 15,
    "editor.fontLigatures": true,
    "editor.codeLens": true,
    "editor.minimap.renderCharacters": false,

    /*
     * WINDOW
     */
    "window.autoDetectColorScheme": true,

    /**
     * EXTENSIONS
     */
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,

    /**
     * EXT:C/C++
     */
    "C_Cpp.clang_format_style": "{BasedOnStyle: LLVM, IndentWidth: 4}",

    /**
     * EXT:VIM
     */
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.normalModeKeyBindingsNonRecursive": [
    ],

    /**
     * EXT:Doxygen Doc
     */
    "doxdocgen.c.commentPrefix": "///",
    "doxdocgen.c.firstLine": "///",
    "doxdocgen.c.lastLine": "///",
    "doxdocgen.c.triggerSequence": "///",
    "doxdocgen.cpp.tparamTemplate": " \\tparam {param} ",
    "doxdocgen.file.fileTemplate": " \\file {name}",
    "doxdocgen.file.versionTag": " \\version 0.1",
    "doxdocgen.generic.authorTag": " \\author {author} ({email})",
    "doxdocgen.generic.briefTemplate": " \\brief {text}",
    "doxdocgen.generic.dateTemplate": " \\date {date}",
    "doxdocgen.generic.paramTemplate": " \\param {param} ",
    "doxdocgen.generic.returnTemplate": " \\return {type} ",
     
}