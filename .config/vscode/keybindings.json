[
    /**
     * View
     */
    { "key": "ctrl+q", "command": "-workbench.action.quickOpenView" },
    { "key": "ctrl+q", "command": "workbench.action.openView" },
    { "key": "ctrl+tab", "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup" },
    { "key": "ctrl+tab", "command": "workbench.action.showEditorsInGroup", "when": "!activeEditorGroupEmpty" },
    { "key": "ctrl+cmd+b", "command": "workbench.action.toggleActivityBarVisibility" },

    /**
     * vscode-neovim
     */
    {
        "command": "vscode-neovim.send",
        "key": "ctrl+]", "args": "<c-]>",
        "when": "editorTextFocus && neovim.mode == insert"
    },
    {
        "command": "vscode-neovim.send",
        "key": "ctrl+shift+]", "args": "<c-}>",
        "when": "editorTextFocus && neovim.mode == insert"
    }
]
